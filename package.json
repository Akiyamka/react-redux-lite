{
  "name": "react-redux-starter",
  "version": "0.0.1",
  "description": "react-redux starter with generators",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "ava --v",
    "start": "export NODE_ENV=development && webpack-dev-server --mode development --host 0.0.0.0 --public",
    "dev": "export NODE_ENV=development && npm install && webpack-dev-server --mode development --host 0.0.0.0",
    "build": "export NODE_ENV=production && npm install && webpack --mode production --display-error-details",
    "analyze": "npx webpack --profile --json > stats.json && npx webpack-bundle-analyzer stats.json",
    "new": "npx plop",
    "commit": "npx git-cz"
  },
  "author": "akiyamka",
  "license": "ISC",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@commitlint/cli": "^7.6.1",
    "@commitlint/config-conventional": "^7.6.0",
    "ava": "^1.4.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^2.2.0",
    "inquirer-recursive": "0.0.6",
    "plop": "^2.3.0",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "svg-react-loader": "^0.4.6",
    "svg-spritemap-webpack-plugin": "^3.3.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "classnames": "^2.2.6",
    "eslint-plugin-react-hooks": "^1.6.0",
    "mapbox-gl": "^0.54.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "unistore": "^3.4.1"
  }
}
